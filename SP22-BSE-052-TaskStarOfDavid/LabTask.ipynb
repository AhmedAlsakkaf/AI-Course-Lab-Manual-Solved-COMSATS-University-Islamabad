   "source": [
    "max = int(input(\"Enter the max number: \"))\n",
    "col = max + max-5\n",
    "mid = col//2\n",
    "\n",
    "\n",
    "\n",
    "for i in range(max):\n",
    "  for j in range (col):\n",
    "    if i==2 or i==(max-3) or i+j==mid or j-i==mid or i-j==2 or j+i==col+1:\n",
    "      print(\"*\", end=\" \")\n",
    "\n",
    "    else:\n",
    "      print(\" \", end=\" \")\n",
    "\n",
    "  print()\n",
    "    \n"
   ]
  }
